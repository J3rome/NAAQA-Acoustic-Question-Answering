{
  "question":{
    "word_embedding_dim":200,
    "rnn_state_size":1024
  },
  "image_extractor":{
    "type":"film_original",
    "out": [128, 128, 128, 128],
    "kernels": [[4, 4], [4, 4], [4, 4], [4, 4]],
    "strides": [2, 2, 2, 2]
  },
  "stem":{
    "spatial_location":true,
    "conv_out":64,
    "conv_kernel":[
      3,
      3
    ]
  },
  "resblock":{
    "conv_out": [64, 64, 64],
    "spatial_location":true,
    "film_projection_type":"plus",
    "kernel1":[
      1,
      1
    ],
    "kernel2":[
      3,
      3
    ]
  },
  "classifier":{
    "spatial_location":true,
    "type":"fcn",
    "global_pool_type":"max",
    "conv_out":128,
    "conv_kernel":[
      1,
      1
    ],
    "no_mlp_units":512,
    "projection_size":512
  },
  "optimizer":{
    "type":"adam",
    "learning_rate":0.0003,
    "clip_val":0.0,
    "weight_decay":5e-6,
    "sgd_momentum":0.0,
    "dropout_drop_prob":0.25,
    "cyclical":{
      "base_learning_rate":0.0001,
      "max_learning_rate":0.05,
      "base_momentum":0.85,
      "max_momentum":0.95,
      "cycle_length":0.1,
      "max_learning_rate_scaling_factor":0.9
    }
  },
  "early_stopping":{
    "enable":true,
    "wait_first_n_epoch":5,
    "stop_threshold":6,
    "min_step":0.0005
  }
}